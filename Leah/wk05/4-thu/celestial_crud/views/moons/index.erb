<h2>Moons</h2>
    <% @moons.each do |moon| %>
        <section class="record">
            <h3><a href="/moon/<%= moon.id %>"><%= moon.name %></a></h3>
            <a href="/moon/<%= moon.id %>">
                <img class="thumb" src="<%= moon.photo_url %>" alt="<%= moon.name %>">
            </a>
        </section>
    <% end %>  